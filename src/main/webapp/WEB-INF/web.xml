<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0"
         metadata-complete="true">
    <filter>
        <filter-name>AccessCounterFilter</filter-name>
        <filter-class>AccessCounter.AccessCounterFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>AccessCounterFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>Servlet.home</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>browse</servlet-name>
        <servlet-class>Servlet.browse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>browse</servlet-name>
        <url-pattern>/browse</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>details</servlet-name>
        <servlet-class>Servlet.details</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>details</servlet-name>
        <url-pattern>/details</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>details_init</servlet-name>
        <servlet-class>Servlet.details_init</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>details_init</servlet-name>
        <url-pattern>/details_init</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>query_init</servlet-name>
        <servlet-class>Servlet.query_init</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>query_init</servlet-name>
        <url-pattern>/query_init</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>deg_cell_type_change</servlet-name>
        <servlet-class>Servlet.deg_cell_type_change</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deg_cell_type_change</servlet-name>
        <url-pattern>/deg_cell_type_change</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>Servlet.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/SearchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DEGServlet</servlet-name>
        <servlet-class>Servlet.DEGServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DEGServlet</servlet-name>
        <url-pattern>/deg</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GeneSearchServlet</servlet-name>
        <servlet-class>Servlet.GeneSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GeneSearchServlet</servlet-name>
        <url-pattern>/gene-search</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GeneDetailsServlet</servlet-name>
        <servlet-class>Servlet.GeneDetailsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GeneDetailsServlet</servlet-name>
        <url-pattern>/gene-details</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IntegrateServlet</servlet-name>
        <servlet-class>Servlet.IntegrateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IntegrateServlet</servlet-name>
        <url-pattern>/integrate</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VisualizationServlet</servlet-name>
        <servlet-class>Servlet.VisualizationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VisualizationServlet</servlet-name>
        <url-pattern>/visualization</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>Servlet.DownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/download-file</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GeoIPServlet</servlet-name>
        <servlet-class>Servlet.GeoIPServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GeoIPServlet</servlet-name>
        <url-pattern>/track</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>Servlet.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GeoDataAPI</servlet-name>
        <servlet-class>web.GeoDataAPI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GeoDataAPI</servlet-name>
        <url-pattern>/api/geo</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
